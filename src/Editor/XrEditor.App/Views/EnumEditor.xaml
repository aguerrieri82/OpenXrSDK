<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:local="clr-namespace:XrEditor;assembly=XrEditor.Core"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <DataTemplate x:Key="EnumEditor" DataType="{x:Type local:IEnumEditor}">
        <ComboBox Padding="8,4,8,4" ItemsSource="{Binding Values}" SelectedValue="{Binding EditValue, Mode=TwoWay}" >
        </ComboBox>
    </DataTemplate>

</ResourceDictionary>