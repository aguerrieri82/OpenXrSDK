<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:local="clr-namespace:XrEditor;assembly=XrEditor.Core"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">



    <SolidColorBrush x:Key="Yellow">#FBC02D</SolidColorBrush>

    <SolidColorBrush x:Key="PrimaryColor">#303F9F</SolidColorBrush>
    <SolidColorBrush x:Key="PrimaryColorContrast">#FFFFFF</SolidColorBrush>
    
    <Style TargetType="Button" x:Key="IconButton">
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="Background" Value="Transparent"/>
    </Style>
  
</ResourceDictionary>