             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Xr.Engine.Editor"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Panels/PropertiesEditor.xaml"/>
                <ResourceDictionary Source="/Panels/SceneView.xaml"/>
                <ResourceDictionary Source="/Editors/FloatEditor.xaml"/>
                <ResourceDictionary Source="/Editors/Vector3Editor.xaml"/>
