<Window x:Class="Xr.Engine.Editor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Xr.Engine.Editor" d:DataContext="{d:DesignInstance Type=local:MainView}"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
  
    <DockPanel>
        <ContentPresenter  Width="250px" DockPanel.Dock="Right" Content="{Binding PropertiesEditor}"/>
        <ContentPresenter Content="{Binding SceneView}"/>
    </DockPanel>
</Window>
